[Service]
<% if @service_config -%>
EnvironmentFile=-<%= @service_config %>
<% end -%>
<% if @storage_config -%>
EnvironmentFile=-<%= @storage_config %>
<% end -%>
# reset the list of commands to start, prior assignments of this option will
# have no effect
ExecStart=
ExecStart=/usr/bin/dockerd $OPTIONS \
      $DOCKER_STORAGE_OPTIONS
